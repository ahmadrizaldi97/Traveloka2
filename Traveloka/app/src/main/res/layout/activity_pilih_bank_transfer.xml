    <ImageView
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="25dp"
        android:src="@drawable/bri"
        android:layout_alignTop="@+id/textView652saa"
