        android:gravity="center_vertical"
        android:textColor="#000000"
        android:layout_marginLeft="20dp"
        android:layout_alignTop="@+id/imageView71s111"
        android:layout_toRightOf="@+id/imageView71s111"
        android:layout_toEndOf="@+id/imageView71s111"
        android:layout_alignBottom="@+id/imageView71s111" />


</RelativeLayout>
