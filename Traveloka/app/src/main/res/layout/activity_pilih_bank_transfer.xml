        android:layout_alignTop="@+id/textView65aiii"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView723" />
    
    <TextView
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:text="BCA"
        android:id="@+id/bcaaa"
        android:textSize="20dp"
        android:gravity="center_vertical"
        android:textColor="#000000"
        android:layout_marginLeft="20dp"
        android:layout_alignTop="@+id/imageView723"
        android:layout_toRightOf="@+id/imageView723"
        android:layout_toEndOf="@+id/imageView723"
        android:layout_alignBottom="@+id/imageView723" />

    <!--dkald-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        android:id="@+id/textView65as"
